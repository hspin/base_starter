{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "useTabs": false,
  "semi": true,
  "bracketSameLine": false,
  "bracketSpacing": true,
  "quoteProps": 'as-needed',
  "arrowParens": 'always',
  "jsxSingleQuote": true,
  "plugins": ["prettier-plugin-tailwindcss"],
}
